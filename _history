false\n}\n\nfunction checkWin(): boolean {\n    for (let y = 0; y < 5; y++) {\n        for (let x = 0; x < 5; x++) {\n            let p = board[y][x]\n            if (p == 0) continue\n            // Horizontal\n            if (x <= 2 && p == board[y][x + 1] && p == board[y][x + 2]) return true\n            // Vertical\n            if (y <= 2 && p == board[y + 1][x] && p == board[y + 2][x]) return true\n            // Diagonal \\\n            if (x <= 2 && y <= 2 && p == board[y + 1][x + 1] && p == board[y + 2][x + 2]) return true\n            // Diagonal /\n            if (x >= 2 && y <= 2 && p == board[y + 1][x - 1] && p == board[y + 2][x - 2]) return true\n        }\n    }\n    return false\n}\n\nfunction switchPlayer() {\n    currentPlayer = currentPlayer == 1 ? 2 : 1\n}\n\ninput.onButtonPressed(Button.A, function () {\n    col = (col + 4) % 5\n    drawBoard()\n})\n\ninput.onButtonPressed(Button.B, function () {\n    col = (col + 1) % 5\n    drawBoard()\n})\n\ninput.onGesture(Gesture.Shake, function () {\n    if (dropToken()) {\n        if (checkWin()) {\n            basic.showString(\"P\" + currentPlayer + \" wins!\")\n            control.reset()\n        } else {\n            switchPlayer()\n        }\n    } else {\n        basic.showString(\"Full!\")\n    }\n    drawBoard()\n})\n\ndrawBoard()\n","README.md":"","pxt.json":"{\n    \"name\": \"test\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}}],"shares":[],"lastSaveTime":1751385744870}